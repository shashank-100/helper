{
  "name": "@helperai/react",
  "version": "0.9.0",
  "description": "React/Next.js integration for Helper chat widget",
  "keywords": [
    "helper",
    "chat",
    "ai",
    "widget",
    "sdk",
    "react",
    "nextjs",
    "customer-service"
  ],
  "author": "Helper (https://helper.ai)",
  "license": "MIT",
  "homepage": "https://helper.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/antiwork/helper.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./auth": {
      "types": "./dist/cjs/auth.d.ts",
      "import": "./dist/esm/auth.mjs",
      "require": "./dist/cjs/auth.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@ai-sdk/react": ">=0.0.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@helperai/client": "workspace:*",
    "@tanstack/react-query": "^5.0.0",
    "react-markdown": "^9.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "esbuild-fix-imports-plugin": "^1.0.21",
    "glob-promise": "^6.0.0",
    "jsdom": "^23.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.3.6",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
