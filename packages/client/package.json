{
  "name": "@helperai/client",
  "version": "0.4.1",
  "description": "Helper API client for integrating customer support features",
  "keywords": [
    "helper",
    "api",
    "client",
    "sdk"
  ],
  "author": "Helper (https://helper.ai)",
  "license": "MIT",
  "homepage": "https://helper.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/antiwork/helper.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.mjs",
      "require": "./dist/auth.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.49",
    "@types/node": "^22.15.3",
    "tsup": "^8.3.6",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "superjson": "^2.2",
    "zod": "^3.24"
  }
}
